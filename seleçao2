seleçao2

package main

import "fmt"

func main() {
	var n1 int
	var n2 int
	var n3 int
	fmt.Print("primeiro numero ")
	fmt.Scan(&n1)
	fmt.Print("segundo numero ")
	fmt.Scan(&n2)
	fmt.Print("terceiro numero ")
	fmt.Scan(&n3)
	if n1 < n2 && n1 < n3 {
		fmt.Print("o menor numero é ", n1)
	} else if n1 < n2 && n1 < n3 {
		fmt.Print("o menor numero é ", n2)
	} else {
		fmt.Print("o menor numero é ", n3)
	}
}

