seleçao6


package main

import "fmt"

func main() {
	var num1, num2 int

	fmt.Print("Digite o primeiro número: ")
	fmt.Scanln(&num1)

	fmt.Print("Digite o segundo número: ")
	fmt.Scanln(&num2)

	if num1 > 0 && num2 > 0 {
		fmt.Printf("A multiplicação de %d e %d é %d.\n", num1, num2, num1*num2)
	} else {
		fmt.Printf("A soma de %d e %d é %d.\n", num1, num2, num1+num2)
	}
}
